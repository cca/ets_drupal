/******************************
 VERTICAL (left/right) menus
******************************/

/* This is the default width of all vertical menus. */
ul.nice-menu-right, ul.nice-menu-left,
ul.nice-menu-right li, ul.nice-menu-left li {
  width: 5.5em;
}

/* VERTICAL menus where submenus pop RIGHT (default). */
ul.nice-menu-right ul {
  width: 5.5em;
  left: 12.5em;
  top: -1px;
}

ul.nice-menu-right ul ul {
  width: 12.5em;
  left: 12.5em;
  top: -1px;
}

ul.nice-menu-right li.menuparent,
ul.nice-menu-right li li.menuparent {
  background: #FFFFFF url(/sites/all/themes/ets/images/arrow-right.png) right center no-repeat;
}

ul.nice-menu-right li.menuparent:hover,
ul.nice-menu-right li li.menuparent:hover {
  background: #FFFFFF url(/sites/all/themes/ets/images/arrow-right.png) right center no-repeat;
}

/* VERTICAL menus where submenus pop LEFT. */
ul.nice-menu-left li ul {
  width: 12.5em;
  left: -12.65em;
  top: -1px;
}

ul.nice-menu-left li ul li ul {
  width: 12.5em;
  left: -12.65em;
  top: -1px;
}

ul.nice-menu-left li.menuparent,
ul.nice-menu-left li li.menuparent {
  background: #FFFFFF url(/sites/all/themes/ets/images/arrow-left.png) left center no-repeat;
}

ul.nice-menu-left li.menuparent:hover,
ul.nice-menu-left li li.menuparent:hover {
  background: #FFFFFF url(/sites/all/themes/ets/images/arrow-left.png) left center no-repeat;
}

ul.nice-menu-left a, ul.nice-menu-left ul a {
  padding-left: 14px;
}

/******************************
 HORIZONTAL (down) menus
******************************/

ul.nice-menu-down {
  	float: left; /* LTR */
  	border: 0;
}

ul.nice-menu-down li {
  	border: none;
}

ul.nice-menu-down li li {
  	border-bottom: 1px dotted #666;
	line-height: 30px;
	height: 30px;
	width: 155px;
	font-size: 11px;
	letter-spacing: .08em;
	font-weight: normal;
	padding-left: 0;
  	margin-left: 0;
}

ul.nice-menu-down li li.last {
	border: none;
}

ul.nice-menu-down ul {
  	left: 0; /* LTR */
  	width: 160px;
  	padding: 10px;
  	background-color: #FFFFFF;
  	top: 15px;
}

ul.nice-menu-down ul li {
  clear: both;
}


ul.nice-menu-down li ul li ul {
  left: 15em; /* LTR */
  top: -15px; /*Adjust this to move up/down position of right-expanded menus*/
}

ul.nice-menu-down .menuparent a {
  padding-right: 15px; /* LTR */
}

ul.nice-menu-down li.menuparent {
  background: #FFFFFF;
}

ul.nice-menu-down li.menuparent:hover {
  background: #FFFFFF;
}

