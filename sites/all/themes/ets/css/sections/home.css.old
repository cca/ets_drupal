#main  {
	margin-top: 25px;
	padding: 0 60px;
	background-image: none;
	float: left;
	width: 1000px;
}

#sidebar-left {
  	margin: 0;
  	background: #EAFF00;
    width: 520px;
    height: 346px;
    padding: 10px 6px;
    float: left;
}

#sidebar-left a {
	padding: 0;
	margin: 0;
}

#sidebar-left p {
	padding: 0;
	margin: 0;
	float: left; 
	clear: both;
}

#sidebar-left h1 {
	position: relative;
	top: 275px;
	left: 5px;
	font-size: 16pt;
}

#content {
  	background: #C8EDFF;
    width: 320px;
    margin: 0 120px 0 0;
    height: 354px;
    overflow: hidden;
    padding: 10px;
    float: right;
}

#front-news {
	width: 320px;
	height: 100px;
	margin: 0 0 15px 0;
	padding: 0;
	float: left;
	position: relative;
	font-size: 12px;
}

#front-news h3 {
	padding: 0;
	margin: 0 0 5px 0;
}

.front-image {
	float: left;
	width: 130px;
	margin: 0;
	padding: 0;
	vertical-align: top;
}

.front-text {
	float: right;
	width: 180px;
	margin: 0;
	padding: 0;
	line-height: 12px;
}

#content a:link {
	color: #000000;
	text-decoration: none;
}

#content a:visited {
	color: #000000;
	text-decoration: none;
}

#content a:hover, a:visited:hover {
	color: #000000;
	text-decoration: underline;
}

#content-bottom {
	padding-left: 50px;
	margin: 0;
	float: left;
	width: 875px;
	height: 37px;
	overflow: hidden;
}

#content-bottom .block {
	margin: 0;
	padding: 0;
}

#content-bottom p {
	margin: 0;
	padding: 0;
}

p {
	font-family: "Georgia", "Times New Roman", "Times", serif;
	font-size: 9pt;
	color: #333333;
}

p.readmore {
	clear: both;
	float: right;
	padding-bottom: 10px;
	padding-right: 15px;
}

h1 {
	font-size: 12pt;
	font-family: "Arial", "Helvetica", sans-serif;
	font-style: normal;
}

h3 {
	font-family: "Arial", "Helvetica", sans-serif;
	padding-top: 5px;
	font-size: 9pt;
}

.twitter-logo {
	vertical-align: top;
	float: left;
	margin-top: 10px;
}

.rotor-items {
	z-index: 0;
}